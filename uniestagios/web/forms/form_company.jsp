<div class="input-field col s12">
    <input  type="hidden" name="perfil" value="company">
</div>

<div class="input-field col s12">
    <input  type="hidden" name="flag" value="cadastrar">
</div>

<div class="input-field col s6">
    <input placeholder="kronamix" id="username" type="text" class="validate" name="username"  focus>
    <label for="username">Nome de usuário</label>
</div>
<div class="input-field col s6">
    <input id="password" type="password" class="validate" name="pass" >
    <label for="password">Password</label>
</div>

<div class="input-field col s12">
    <input  id="cnpj" type="text" class="validate" name="cnpj" >
    <label for="cnpj">CNPJ</label>
</div>
<div class="input-field col s12">
    <input id="razaoSocial" type="text" class="validate" name="razaoSocial" >
    <label for="razaoSocial">Razão Social</label>
</div>

<div class="input-field col s12">
    <input id="nameCompany" type="text" class="validate" name="nameCompany" >
    <label for="nameCompany">Nome Fantasia</label>
</div>

<div class="input-field col s12">
    <input placeholder="exemplo@exemplo.com" id="emailCompany" type="email" name="email" class="validate" data-error="Invalido" data-success="ok!" >
    <label for="emailCompany">E-mail</label>
</div>

<div class="input-field col s12">
    <input placeholder="(DD) + Telefone" id="phoneCompany" type="text"  name="phoneCompany" class="validate" >
    <label for="phoneCompany">Telefone</label>
</div>

<div class="input-field col s12">
    <input  id="responsible" type="text" class="validate" name="responsible" >
    <label for="responsible">Nome do Responsavel</label>
</div>



<div class="input-field col s12">
    <input  id="companyBranch" type="text" class="validate" name="companyBranch" >
    <label for="companyBranch">Ramo de Atividades</label>
</div>
